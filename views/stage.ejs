<!DOCTYPE html>
<html lang="en">
    <head>
        <title> <%= title %> </title>
        <link rel="stylesheet" href="styles/main.css">
    </head>

    <body>
        <%- include("particles/nav.ejs") %>

        <div class="threeDivs">
        <div class="leftImage"><img class="imgBorderSizes" alt="" src="images/picture13.png"></div>
        <div class="rightImage"><img class="imgBorderSizes" alt="" src="images/picture12.png"></div>
        <div class="content">
            <h1 class="title" id="underline">The list of stages for "The BIG Festival"</h1>
            <% stages.forEach(row=>{ %>
                <div id="lineup">
                    <ul class="tablerow">
                        <li>Stage <%= row.stageID %> Name: <%= row.stageName %></li>
                        <li>Stage Genre: <%= row.stageGenre %></li>
                        <li>Stage Location: <%= row.stageLocation %></li>
                    </ul>
                </div>
            <% }); %>
        </div>
        </div>
        </div>
    
    </body>


</html>